<script type="text/javascript">
$(document).ready(function(){

function ImageSwitcher(choices, i) {
  i = 0;
  
  this.Next = function() {
    hide_current_image();
    show_next_image();
    placeholder.style.visibility = "hidden";
  }
  
  var hide_current_image = function() {
    if(choices){
      choices[i].style.visibility = "hidden";
      i += 1;
    }
  }
  var show_next_image = function() {
    if(choices){
      if(i == (choices.length)) {
        i = 0;
      }
      choices[i].style.visibility = "visible";
    }
  }
}
  
    var tribe = $(".tribe");
  var special = $(".special");
  var breed = $(".breed");
  var mouth = $(".mouth");
  var eyebrows = $(".eyebrows");
  var eyes = $(".eyes");
  var outfit = $(".outfit");
  var head = $(".head");
  var toy = $(".toy");
  var pfp = $(".pfp");

  var tribe_picker = new ImageSwitcher(tribe);
  document.getElementById("tribe_button").onclick = function() { tribe_picker.Next(); };
  
  var special_picker = new ImageSwitcher(special);
  document.getElementById("special_button").onclick = function() {special_picker.Next(); };
  
  var breed_picker = new ImageSwitcher(breed);
  document.getElementById("breed_button").onclick = function() {breed_picker.Next(); };

  var mouth_picker = new ImageSwitcher(mouth);
  document.getElementById("mouth_button").onclick = function() {mouth_picker.Next(); };

  var eyebrows_picker = new ImageSwitcher(eyebrows);
  document.getElementById("eyebrows_button").onclick = function() {eyebrows_picker.Next(); };

  var eyes_picker = new ImageSwitcher(eyes);
  document.getElementById("eyes_button").onclick = function() {eyes_picker.Next(); };

  var outfit_picker = new ImageSwitcher(outfit);
  document.getElementById("outfit_button").onclick = function() {outfit_picker.Next(); };

  var head_picker = new ImageSwitcher(head);
  document.getElementById("head_button").onclick = function() {head_picker.Next(); };

  var toy_picker = new ImageSwitcher(toy);
  document.getElementById("toy_button").onclick = function() {toy_picker.Next(); };

  var pfp_picker = new ImageSwitcher(pfp);
  document.getElementById("pfp_button").onclick = function() {pfp_picker.Next(); };

});

  $("#tribe_button").click(function(){
  $("#tribe-menu").css("visibility", "visible"); });
</script>
